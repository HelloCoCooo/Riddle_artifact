group AddressDAO;

createAddress() ::= <<
    INSERT INTO address (address_id, company, full_name, street, street_complement, zip, city, country)
    VALUES (:address.id,
            :address.company,
            :address.fullName,
            :address.street,
            :address.streetComplement,
            :address.zip,
            :address.city,
            :address.country)
>>
